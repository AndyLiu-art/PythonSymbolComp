   },
   "outputs": [],
   "source": [
    "from sympy import *\n",
    "# 定义变量\n",
    "x, y, z = symbols(\"x y z\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "d1ae4966-17b5-4138-9c34-edd3149a84ce",
   "metadata": {
    "tags": []
   },
   "outputs": [
    {
     "data": {
      "text/latex": [
       "$\\displaystyle y{\\left(x \\right)} = C_{1} \\sin{\\left(3 x \\right)} + C_{2} \\cos{\\left(3 x \\right)}$"
      ],
      "text/plain": [
       "Eq(y(x), C1*sin(3*x) + C2*cos(3*x))"
      ]
     },
     "execution_count": 2,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# 定义函数\n",
    "y = Function(\"y\")\n",
    "# Solve the ODE\n",
    "result = dsolve(Derivative(y(x), x, x) + 9*y(x), y(x))\n",
    "result"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "ff1ab721-7cf2-4575-bd67-a2f7093b20be",
