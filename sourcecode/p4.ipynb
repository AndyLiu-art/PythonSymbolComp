   "source": [
    "import numpy as np\n",
    "a = np.arange(10) \n",
    "# 定义表达式\n",
    "expr = sin(x)\n",
    "# 定义函数，输出结果为numpy的数组类型\n",
    "f = lambdify(x, expr, \"numpy\")\n",
    "f(a) "
   ]
